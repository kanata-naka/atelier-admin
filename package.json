{
  "name": "atelier-admin",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev": "node server.js",
    "clean": "rimraf \"dist/functions/**\" && rimraf \"dist/public/**\"",
    "build": "cpx \"functions/index.js\" \"dist/functions\" -C && next build",
    "copy-resources": "cpx \"public/**/*.*\" \"dist/public\" -C && cpx \"dist/functions/build/static/**/*.*\" \"dist/public/_next/static\" && cpx \"config/**/*.*\" \"dist/functions/config\"",
    "install-dependencies": "cpx \"*{package.json,package-lock.json}\" \"dist/functions\" && cd \"dist/functions\" && npm install",
    "preserve": "npm run build && npm run copy-resources && npm run install-dependencies",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "deploy": "firebase deploy --interactive",
    "prettier": "prettier src/**/*.js --write"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cpx": "^1.5.0",
    "cross-env": "^7.0.0",
    "firebase-functions-test": "^0.1.7",
    "firebase-tools": "^7.13.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2"
  },
  "prettier": {
    "semi": false,
    "jsxBracketSameLine": true
  },
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "config": "^3.2.5",
    "decimal.js": "^10.2.0",
    "firebase": "^7.8.2",
    "firebase-functions": "^3.3.0",
    "moment": "^2.24.0",
    "next": "^9.2.2",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dnd-cjs": "^9.5.1",
    "react-dnd-html5-backend-cjs": "^9.5.1",
    "react-dom": "^16.12.0",
    "react-form": "^4.0.1",
    "react-notification-system": "^0.3.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-form": "^8.3.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "uuid": "^3.4.0"
  }
}
